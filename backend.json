{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "patternProperties": {
    "^[a-zA-Z0-9]+$": {
      "type": "object",
      "properties": {
        "image": {
          "type": "string",
          "format": "uri",
          "description": "URL to the image"
        },
        "name": {
          "type": "string",
          "description": "Name associated with the image"
        }
      },
      "required": ["image"],
      "additionalProperties": false
    }
  },
  "additionalProperties": false
}
